<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:caldroid="http://schemas.android.com/apk/res-auto">

    <item android:drawable="@mipmap/ic_event_blue_light" android:state_pressed="false" caldroid:state_date_disabled="true" caldroid:state_date_today="true"/>

    <item android:drawable="@mipmap/ic_event_blue_dark" android:state_pressed="false" caldroid:state_date_today="true" caldroid:state_date_selected="true" caldroid:state_date_prev_next_month="true"/>
    <item android:drawable="@mipmap/ic_event_blue_light" android:state_pressed="false" caldroid:state_date_today="true" caldroid:state_date_prev_next_month="true"/>

    <item android:drawable="@mipmap/ic_event_blue_dark" android:state_pressed="false" caldroid:state_date_today="true" caldroid:state_date_selected="true"/>
    <item android:drawable="@mipmap/ic_event_blue_middle" android:state_pressed="false" caldroid:state_date_today="true"/>

    <item android:drawable="@drawable/caldroid_custom_disabled_cell" caldroid:state_date_disabled="true"/>
    <item android:drawable="@color/background" android:state_pressed="true"/>
    <item android:drawable="@color/background" android:state_pressed="false"/>

